FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
    volScalarFieldValue T 0
    //volScalarFieldValue p 100000
);

regions
(
    boxToCell
    {
        box (-0.5 -1 -1) (0.5 1 1);
        fieldValues
        (
            volScalarFieldValue T 1.0
            //volScalarFieldValue p 10000
        );
    }
);


